# Staging Environment Configuration
VITE_APP_ENVIRONMENT=staging

# Appwrite Configuration
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
VITE_APPWRITE_PROJECT_ID=your-staging-project-id

# Appwrite Database Configuration
VITE_APPWRITE_DATABASE_ID=staging-database-id

# Appwrite Collection IDs
VITE_APPWRITE_USERS_COLLECTION_ID=staging-users-collection
VITE_APPWRITE_RESUMES_COLLECTION_ID=staging-resumes-collection
VITE_APPWRITE_SESSIONS_COLLECTION_ID=staging-sessions-collection
VITE_APPWRITE_INTERACTIONS_COLLECTION_ID=staging-interactions-collection

# Appwrite Storage Configuration
VITE_APPWRITE_STORAGE_BUCKET_ID=staging-storage-bucket

# AI API Configuration
VITE_AI_API_BASE_URL=https://your-staging-ai-api.com
VITE_AI_API_KEY=staging-api-key

# Application Configuration
VITE_APP_NAME=InterviewPrep AI (Staging)
VITE_APP_VERSION=1.0.0-staging
VITE_APP_DEBUG=true

# OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-staging-google-client-id
VITE_LINKEDIN_CLIENT_ID=your-staging-linkedin-client-id

# Feature Flags
VITE_ENABLE_SPEECH_RECOGNITION=true
VITE_ENABLE_OAUTH_LOGIN=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_TOOLS=true

# Staging specific
VITE_MOCK_AI_RESPONSES=false
VITE_LOG_LEVEL=info

# Error Monitoring
VITE_SENTRY_DSN=your-staging-sentry-dsn
VITE_ENABLE_ERROR_REPORTING=true

# Analytics
VITE_GA_TRACKING_ID=your-staging-google-analytics-id