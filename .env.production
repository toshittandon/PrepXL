# Production Environment Configuration
VITE_APP_ENVIRONMENT=production

# Appwrite Configuration
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
VITE_APPWRITE_PROJECT_ID=your-production-project-id

# Appwrite Database Configuration
VITE_APPWRITE_DATABASE_ID=production-database-id

# Appwrite Collection IDs
VITE_APPWRITE_USERS_COLLECTION_ID=production-users-collection
VITE_APPWRITE_RESUMES_COLLECTION_ID=production-resumes-collection
VITE_APPWRITE_SESSIONS_COLLECTION_ID=production-sessions-collection
VITE_APPWRITE_INTERACTIONS_COLLECTION_ID=production-interactions-collection

# Appwrite Storage Configuration
VITE_APPWRITE_STORAGE_BUCKET_ID=production-storage-bucket

# AI API Configuration
VITE_AI_API_BASE_URL=https://your-production-ai-api.com
VITE_AI_API_KEY=production-api-key

# Application Configuration
VITE_APP_NAME=InterviewPrep AI
VITE_APP_VERSION=1.0.0
VITE_APP_DEBUG=false

# OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-production-google-client-id
VITE_LINKEDIN_CLIENT_ID=your-production-linkedin-client-id

# Feature Flags
VITE_ENABLE_SPEECH_RECOGNITION=true
VITE_ENABLE_OAUTH_LOGIN=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_TOOLS=false

# Production specific
VITE_MOCK_AI_RESPONSES=false
VITE_LOG_LEVEL=error

# Error Monitoring
VITE_SENTRY_DSN=your-sentry-dsn
VITE_ENABLE_ERROR_REPORTING=true

# Analytics
VITE_GA_TRACKING_ID=your-google-analytics-id
VITE_HOTJAR_ID=your-hotjar-id